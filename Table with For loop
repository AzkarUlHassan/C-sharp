namespace Azkarii
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Please Enter your number to print table");
            int n;
            n = Convert.ToInt32(Console.ReadLine());
            for(int i = 1; i <= 10; i++)
            {
                Console.WriteLine(n + " X " + i + " = " + (i * n));
            }
            Console.ReadLine();

            
        }
    }
}
